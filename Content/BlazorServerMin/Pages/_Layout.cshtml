@using Microsoft.AspNetCore.Components.Web
@namespace BlazorServerMin.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="~/" />
@*#if (Framework == "net8.0")*@
    <link rel="stylesheet" href="css/blazor-ui.css" />
    <link rel="stylesheet" href="BlazorServerMin.styles.css" />
@*#else*@
    <link rel="stylesheet" href="css/blazor-ui.css" asp-append-version="true" />
    <link rel="stylesheet" href="BlazorServerMin.styles.css" asp-append-version="true" />
@*#endif*@
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
</head>
<body>
    @RenderBody()

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
</body>
</html>
